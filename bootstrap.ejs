<script type="text/javascript">
        var container = document.getElementById("container");
        var graphData = new Array();
        var tests = <%-JSON.stringify(tests) %>;

        function downloadLog(name) {

            var logs = tests[name].logs;
            var logTXT = '';

            for (var i = 0; i < logs.length; i++) {
                logTXT += logs[i].time + '\t' + logs[i].message + '\n';
            }

            window.open('data:download/plain;charset=utf-8,' + encodeURI(logTXT), '_blank');
        }

        for (var test in tests) {
            var name = tests[test].name;
            var points = tests[test].points;
            var d1 = new Array();

            for (var i = 0; i < points.length; i++) {
                var point = points[i];
                d1.push([point.x, point.y]);
            }

            graphData.push({data: d1, label:name, lines: { show: true }, points: { show: true }});

        }

        function drawGraph() {

            return Flotr.draw(container, graphData,
                {
                    HtmlText: false,
                    title: '<%= name %>',
                    xaxis: {
                        minorTickFreq: 4,
                        title: '<%= Xaxis %>'
                    },
                    yaxis: {
                        title: '<%= Yaxis %>'
                    },
                    grid: {
                        minorVerticalLines: true
                    },
                    mouse: {
                        track: true,
                        relative: true
                    }
                });
        }

        var graph = drawGraph();

        var downloadGraph = document.getElementById("downloadGraph");
        downloadGraph.onclick = function(){
            graph.download.saveImage('png');
        };
    </script>
<script type="text/javascript">
    var containerCPU = document.getElementById("containerCPU"),
            containerMEM = document.getElementById("containerMEM"),
            downloadCPU = document.getElementById("downloadCPU"),
            downloadMEM = document.getElementById("downloadMEM"),
            graphCPU;

    var displayGraph = function(data, title, xaxisTitle, yaxisTitle, yMax, yMin, container, downloadButton) {
        var options, graph, graphData, xMin = Number.MAX_VALUE, xMax = Number.MIN_VALUE;

        graphData = new Array();
        for (var i = 0; i < data.length; i++) {
            graphData.push({data: data[i].data, label: data[i].name, lines: { show: true }, points: { show: true }});

            for (var j = 0; j < data[i].data.length; j++) {
                if (data[i].data[j][0] < xMin) {
                    xMin = data[i].data[j][0];
                }

                if (data[i].data[j][0] > xMax) {
                    xMax = data[i].data[j][0];
                }
            }

        }
        options = {
            title: title,
            xaxis: {
                min: xMin,
                max: 10,
                showLabels: false,
                minorTickFreq: 4,
                title: xaxisTitle
            },
            yaxis: {
                min: yMax,
                max: yMin,
                showLabels: true,
                minorTickFreq: 4,
                title: yaxisTitle,
                tickDecimals: 2,
                min: 0
            },
            grid: {
                minorVerticalLines: true
            },
            mouse : {
                track : true,
                relative : true,
                trackFormatter: function(obj){
                    return obj.y;
                }
            },
            HtmlText: false,
            legend: {
                position: 'nw'
            }
        };
        graph = drawGraph(container);

        // Draw graph with default options, overwriting with passed options
        function drawGraph (container, opts) {

            // Clone the options, so the 'options' variable always keeps intact.
            var o = Flotr._.extend(Flotr._.clone(options), opts || {});

            // Return a new graph.
            return Flotr.draw(container, graphData, o);
        }

        function initializeDrag (e) {
            start = graph.getEventPosition(e);
            Flotr.EventAdapter.observe(document, 'mousemove', move);
            Flotr.EventAdapter.observe(document, 'mouseup', stopDrag);
        }

        function move (e) {
            var
                    end     = graph.getEventPosition(e),
                    xaxis   = graph.axes.x,
                    offset  = start.x - end.x,
                    min, max;

            if (xaxis.min + offset < xMin) {
                min = xaxis.min;
                max = xaxis.max;
            } else if (xaxis.max + offset > xMax) {
                min = xaxis.min;
                max = xaxis.max;
            } else {
                min = xaxis.min + offset;
                max = xaxis.max + offset;
            }

            graph = drawGraph(container, {
                xaxis : {
                    showLabels: false,
                    min : min,
                    max : max,
                    minorTickFreq: 4,
                    title: xaxisTitle
                }
            });

            // @todo: refector initEvents in order not to remove other observed events
            Flotr.EventAdapter.observe(graph.overlay, 'mousedown', initializeDrag);
        }

        function stopDrag () {
            Flotr.EventAdapter.stopObserving(document, 'mousemove', move);
        }

        Flotr.EventAdapter.observe(graph.overlay, 'mousedown', initializeDrag);

        if (downloadButton) {
            downloadButton.onclick = function() { graph.download.saveImage('png'); };
        }
    }

    <% if(!CPU_Mem) {%>

        containerCPU.innerHTML = "<br/><div class='alert'>" +
                "<strong>Warning</strong> No monitors found to get CPU" +
                "</div>"
        containerMEM.innerHTML = "<br/><div class='alert'>" +
                "<strong>Warning</strong> No monitors found to get Memory" +
                "</div>"

        window.onresize = function() {
            graph = drawGraph();
        };
    <% }else{ %>

        var persCPU = [];
        var persMem = [];

        var data = <%-CPU_Mem %>;
        console.log(data);
        for (var hostAndName in data) {
            var cpu = [];
            var mem = [];
            var eachArray = data[hostAndName];
            for (var i=0; i < eachArray.length; i++){
                cpu.push([i,eachArray[i].cpu]);
                mem.push([i,eachArray[i].memory / 1024])
            }
            console.log(cpu);
            persCPU.push({ name: hostAndName, data: cpu });
            persMem.push({ name: hostAndName, data: mem });
        }
        displayGraph(persCPU, 'CPU Graph', 'Time', 'Percentage', 0, 100, containerCPU, downloadCPU);
        displayGraph(persMem, 'MEM Graph', 'Time', 'MB', null, null, containerMEM, downloadMEM);

        window.onresize = function() {
            graph = drawGraph();
            displayGraph(persCPU, 'CPU Graph', 'Time', 'Percentage', 0, 100, containerCPU, downloadCPU);
            displayGraph(persMem, 'MEM Graph', 'Time', 'MB', null, null, containerMEM, downloadMEM);
        };
    <% } %>

</script>

</body>
</html>