<body>
<br/>

<div class="container-fluid">
    <div class="row-fluid">
        <div class="span12">
            <div class="hero-unit">
                <h1><%= name %></h1>

                <p><%= description %></p>

            </div>
            <div class="row-fluid">
                <div class="span7" style="overflow: hidden;height: 100%;">
                    <div class="hero-unit" style="height: 80%;">
                        <div id="container" style="height: 500px"></div>
                        <button type="button" class="btn btn-primary" id="downloadGraph">Download</button>
                    </div>
                </div>
                <div class="span5" style="height: 100%;">
                    <div class="hero-unit" style="height: 80%;">
                        <div class="container-fluid">
                            <table style="width: 100%">
                                <tbody><tr>
                                    <td>
                                        <h1>CPU</h1>
                                    </td>
                                    <td>
                                        <button type="button" class="btn btn-primary pull-right" id="downloadCPU">Download</button>
                                    </td>
                                </tr>
                                </tbody></table>
                            <div id="containerCPU" style="height: 200px;"></div>
                        </div>
                        <div class="container-fluid">
                            <table style="width: 100%">
                                <tbody><tr>
                                    <td>
                                        <h1>MEM</h1>
                                    </td>
                                    <td>
                                        <button type="button" class="btn btn-primary pull-right" id="downloadMEM">Download</button>
                                    </td>
                                </tr>
                                </tbody></table>
                            <div id="containerMEM" style="height: 200px;"></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row-fluid">
                <div class="span12">
                <div class="hero-unit mem_cpu">
                    <div class="container-fluid">
                    <h2>Log</h2>
                        <div class="tabbable"> <!-- Only required for left/right tabs -->
                            <ul class="nav nav-tabs" id="tab-titles">
                                <%var i=0; for (var test_name in tests) { %>
                                <li <%if (i===0){ %> class="active" <% } %>><a href="#<%= test_name %>" data-toggle="tab"><%= test_name %></a></li>
                                <% i++} %>
                            </ul>
                            <div class="tab-content">
                                <% var i=0; for (var test_name in tests) { %>
                                <div class="tab-pane <%if (i===0){ %> active <% } %>" id="<%= test_name %>">
                                    <div class="scrollable">
                                        <table class="table table-striped">
                                            <thead>
                                            <tr>
                                                <th>Timestamp</th>
                                                <th>Message</th>
                                            </tr>
                                            </thead>
                                            <tbody>
                                            <% for(var i = 0; i < tests[test_name].logs.length; i++){ %>
                                            <tr>
                                                <td><%= tests[test_name].logs[i].time %></td>
                                                <td><%= tests[test_name].logs[i].message %></td>
                                            </tr>
                                            <% } %>
                                            </tbody>
                                        </table>
                                    </div>
                                    <br />
                                    <button type="button" class="btn btn-primary" onclick="downloadLog('<%= test_name %>')">Download</button>
                                </div>
                                <% i++; } %>
                            </div>
                        </div>
                    <br/>
                </div>
            </div>
                    </div>
    </div>
</div>
<script type="text/javascript" src="https://raw.github.com/HumbleSoftware/Flotr2/master/flotr2.min.js"></script>
